<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th scope="col">title</th>
    <th scope="col">band</th>
    <th scope="col">price</th>
    <th scope="col">updated_at</th>
  </tr>
  </thead>
  <tbody>
  <% products.each do |p| %>
    <tr>
      <th scope="row"><%= link_to p.title, admins_product_path(p) %></th>
      <td><%= p.band %></td>
      <td><%= p.price %></td>
      <td><%= p.updated_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>