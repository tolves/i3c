<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th scope="col">id</th>
    <th scope="col">whodunnit</th>
    <th scope="col">event</th>
    <th scope="col">object</th>
    <th scope="col">created_at</th>
  </tr>
  </thead>
  <tbody>
  <% @histories.each do |h| %>
    <tr>
      <th scope="row"><%= h.id %></th>
      <td><%= h.whodunnit %></td>
      <td><%= h.event %></td>
      <td><%= h.object %></td>
      <td><%= h.created_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>